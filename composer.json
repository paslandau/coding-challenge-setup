{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "google/cloud-bigquery": "^v1.24.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^v10.5.1"
    },
    "require-dev": {
        "malukenho/mcbumpface": ">1.2.0",
        "phpunit/phpunit": "^10.0.19",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.7.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "malukenho/mcbumpface": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CodingChallenge\\": "domain/CodingChallenge/src"
        },
        "classmap": [
            "database/migrations",
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "CodingChallenge\\Test\\": "domain/CodingChallenge/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "show-help": [
            "echo 'See Makefile!'",
            "make"
        ]
    }
}
